@using MapLab.Services.Contracts
@using MapLab.Services.Models
@using MapLab.Web.Models.Maps
@using Newtonsoft.Json.Linq
@model MapComponentViewModel
@inject IMapsService _mapsService;
@{
    Layout = "_MapLayout";
    ViewData["Title"] = Model.Name;
}

<component type="typeof(MapComponent)"
           render-mode="ServerPrerendered"
           param-Map="Model" />

@section Styles {
    <link rel="stylesheet" href="~/css/map.css" asp-append-version="true" />
}

@section Scripts {
    <script src="~/lib/d3/d3.js" asp-append-version="true"></script>
    <script src="~/js/d3/map.js" asp-append-version="true"></script>
}
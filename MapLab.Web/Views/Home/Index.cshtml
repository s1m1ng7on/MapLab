@using MapLab.Web.Models.Shared

<partial name="_HeroCarouselPartial" />
<div class="container card-container mt-n5">
    <div class="row">
        <div class="col-md-4">
            <div class="card text-center">
                <div class="card-body">
                    <div class="card-icon mb-3">
                        <img src="path/to/icon1.png" alt="Icon 1" class="img-fluid">
                    </div>
                    <h3 class="card-title">Create Interactive Maps</h3>
                    <p class="card-text">Build custom maps effortlessly with our user-friendly tools. Add markers, draw shapes, and visualize data with ease.</p>
                </div>
            </div>
        </div>
        <div class="col-md-4">
            <div class="card text-center">
                <div class="card-body">
                    <div class="card-icon mb-3">
                        <img src="path/to/icon2.png" alt="Icon 2" class="img-fluid">
                    </div>
                    <h3 class="card-title">Analyze & Visualize Data</h3>
                    <p class="card-text">Bring your data to life with heatmaps, layers, and advanced styling options. Gain insights with powerful mapping analytics.</p>
                </div>
            </div>
        </div>
        <div class="col-md-4">
            <div class="card text-center">
                <div class="card-body">
                    <div class="card-icon mb-3">
                        <img src="path/to/icon3.png" alt="Icon 3" class="img-fluid">
                    </div>
                    <h3 class="card-title">Collaborate & Share</h3>
                    <p class="card-text">Connect with the community by sharing your maps in the Community section. Discover, explore, and contribute to interactive maps from other creators.</p>
                </div>
            </div>
        </div>
    </div>
</div>
